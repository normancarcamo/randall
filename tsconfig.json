{
  "compilerOptions": {
    "plugins": [
      {
        "name": "typescript-styled-plugin",
        "tags": ["styled", "css", "sty"]
      }
    ],
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "~root/*": ["./*"],
      "~public/*": ["public/*"],
      "~static/*": ["public/static/*"],
      "~src/*": ["src/*"],
      "~components/*": ["src/components/*"],
      "~lib/*": ["src/lib/*"],
      "~pages/*": ["src/pages/*"],
      "~app/*": ["src/app/*"],
      "~redux/*": ["src/redux/*"],
      "~styles/*": ["src/styles/*"],
      "~server/*": ["src/server/*"]
    }
  },
  "exclude": ["node_modules"],
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "src/pages/index.tsx.bk"]
}
